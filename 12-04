void main() {

  print('Hello world!');

  var city = "Bangalore";

  print(city);

  greet();

  add(9,7);

  greater(2,4);

}

void greet() {

print('Hello And welcome');

}

void add(var num1,num2) { 

  var sum = num1 + num2;

  print(sum);

  

}

// All arithmetic operation functions 

void greater(num3, num4) {

  if(num3>num4) 

    print(' num3 is Greater');
   else 
   print(' num4 is Greater');

}
